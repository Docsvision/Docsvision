{
	"index_name": "help-docsvision",
	"start_urls": [
		"https://help.docsvision.com/"
	],
	"sitemap_urls": [
		"https://help.docsvision.com/sitemap.xml"
	],
	"selectors": {
		"lvl0": {
			"global": true,
			"selector": "//meta[@name=\"context\"]",
			"type": "xpath"
		},
		"lvl1": ".doc > h1.page",
		"lvl2": ".doc .sect1 > h2:first-child",
		"lvl3": ".doc .sect2 > h3:first-child",
		"lvl4": ".doc .sect3 > h4:first-child",
		"lvl5": ".doc .sidebarblock > .content > .title",
		"text": ".doc p, .doc dt, .doc td.content, .doc th.tableblock"
	}
}